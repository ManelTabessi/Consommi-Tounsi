<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="76x76" href="/Content/img/apple-icon.png">
    <link rel="icon" type="image/png" href="/Content/img/favicon.png">
    <title>
        Consommi Tounsi
    </title>
    <!--     Fonts and icons     -->
    <!-- Nucleo Icons -->
    <link href="/Content/nucleo-icons.css" rel="stylesheet" />
    <!-- CSS Files -->
    <link href="/Content/black-dashboard.css?v=1.0.0" rel="stylesheet" />
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="/Content/demo.css" rel="stylesheet" />
    
</head>

@model Consommi_Tounsi.Models.publication

@{
    ViewBag.Title = "Accepter";
}
<div style="margin-left:10px">
    <h4 style="color:white"> @Html.ActionLink("Back to List", "IndexAdmin")</h4>
</div>

<center><h3><img src="~/Content/img/accepted.png" width="50px" />Are you sure you want to Accept this publication ?</h3></center>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })


<div class="form-actions no-color" style="margin-left:700px">
    <input type="submit" value="yes" class="btn btn-default" style="color:#86c523" />
</div>
     
  
}

</html>
